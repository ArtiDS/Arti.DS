#!/bin/bash

function colors {
  GREEN="\e[32m"
  YELLOW="\e[33m"
  RED="\e[39m"
  NORMAL="\e[0m"
}

colors

echo -e "${BLUE}█████╗░░█████╗░██╗░░░██╗██████╗░██╗░░░░░███████╗  ████████╗░█████╗░██████╗░${NORMAL}"             ███
